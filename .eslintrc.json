{
    "env": {
        "browser": true,
        "es2021": true
    },
    "presets": [
       "@babel/react" , 
       "@babel/env" , 
   ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",    
	"@babel/plugin-proposal-class-properties"
    ],
    "rules": {
		"react/prop-types": 0,
		"react/no-unknown-property": 0,
		"no-case-declarations": 0
	},
	"settings": {
		"react": {
		  "version": "detect"
		}
	  }
}
